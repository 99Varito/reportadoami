
@{
    ViewBag.Title = "IndexSinCovid";
}

<h2>IndexSinCovid</h2>

<table>
    <thead>
        <tr>
            <th>Folio</th>
            <th>Nombre paciente</th>
            <th>Fecha alta</th>
            <th>Descripción</th>
            <th>Comentarios</th>
            <th>Cantidad</th>
            <th>Costo</th>
            <th>OFNa</th>
            <th>Servicio</th>
            <th>Examen</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Folio</td>
                <td>@item.Nombre_paciente</td>
                <td>@item.Fecha_alta</td>
                <td>@item.Perf_descripcion</td>
                <td>@item.comentarios</td>
                <td>@item.Cantidad</td>
                <td>@item.Costo</td>
                <td>@item.OFNa</td>
                <td>@item.servicio</td>
                <td>@item.NOMBRE_EXAMEN</td>
            </tr>
        }
    </tbody>
</table>

