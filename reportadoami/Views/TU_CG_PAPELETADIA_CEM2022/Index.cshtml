@model List<reportadoami.Models.ViewModels.PapeletadiariaCemViewModel>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<h2>papeletadiaCem2</h2>
<div class="row">

    <div class=" col-md-12">
        <table>


            <tr>

                <td>Folio</td>

                <td>Nombre</td>

                <td>Preferencia</td>

                <td>Comentarios</td>

                <td>Nombre Examen</td>

            </tr>
            @foreach (var oElemento in Model)
            {
                <tr>

                    <td>@oElemento.Folio</td>
                    <td>@oElemento.FECHA_ALTA</td>
                    <td>@oElemento.NOMBRE_PACIENTE</td>

                    <td>@oElemento.PERF_DESCRIPCION</td>

                    <td>@oElemento.Comentarios</td>



                </tr>


            }

        </table>


    </div>


</div>

